[sub_resource type="GDScript" id=2]
script/source = "extends Spatial


var col:CollisionShape
func _ready():
	connect(\"visibility_changed\",self,\"_on_visible\")
	col = $Area/CollisionShape
	col.disabled = true
	
func _on_visible():
	if visible:
		col.disabled = false
	else:
		col.disabled = true
	pass

func enable_spin(enabled:bool):
	col.disabled = !enabled
"

[sub_resource type="GDScript" id=3]
script/source = "extends Area

func _ready():
	connect(\"body_entered\",self,\"_on_body_entered\")
	
func _on_body_entered(body:Node):
	print(\"attacked: \",str(body.name))
	if (body.is_in_group(\"crate\")):
		body._on_Attacked()
"

