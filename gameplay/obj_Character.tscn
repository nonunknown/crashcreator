script/source = "
extends Position3D

export var offset:Vector3
var character:Character
var parent
func _ready():
	character = get_parent()
	parent = character.get_parent()
	character.cam_target = self
	character.call_deferred(\"remove_child\",self)
	parent.call_deferred(\"add_child\",self)

func _process(delta):
	translation = character.get_global_transform().origin + offset
"

