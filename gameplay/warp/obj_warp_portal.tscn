		1:
			expand()
			yield(get_tree().create_timer(1,false),\"timeout\")
			contract()
		0:
			contract()



# Called when the node enters the scene tree for the first time.
func expand():
	process_material.set(\"scale\",curve_in.interpolate(1))
	$portal_expand.play()
	yield(get_tree().create_timer(.2,false),\"timeout\")
	$portal_loop.play()
	$rays.visible = true
	pass # Replace with function body.
	
func contract():
	$portal_loop.stop()
	$portal_contract.play()
	$rays.visible = false
	process_material.set(\"scale\",curve_out.interpolate(1))
	pass


