[application]

config/name="Crash Bandicoot Worlds"
config/description="Crash Fan game, based on psx era for fans ;P"
run/main_scene="res://Menu/Intro.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon_256.ico"

[autoload]

FileManager="*res://scripts/Singletons/FileManager.gd"
LoadingManager="*res://scripts/Singletons/LoadingManager.gd"
IDTable="*res://scripts/Singletons/IDTable.gd"
GameManager="*res://scripts/Singletons/GameManager.gd"
Utils="*res://scripts/Singletons/Utils.gd"
SelectionManager="*res://scripts/Editor/SelectionManager.gd"

[compression]

formats/zlib/compression_level=3
formats/gzip/compression_level=3

[display]

window/size/width=720
window/size/height=480
window/size/resizable=false
window/vsync/use_vsync=false
mouse_cursor/custom_image="res://cursor/normal.png"
window/stretch/aspect="keep"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "SIsilicon.3d.text", "simple_water" )

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

